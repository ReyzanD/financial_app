            {
              "id": "income", 
              "type": "radio_option",
              "label": "Income",
              "icon": "income",
              "color": "#2ED573"
            }
          ]
        },
        {
          "id": "category_selection",
          "type": "dropdown",
          "label": "Category",
          "options": [
            "Food & Dining",
            "Transportation", 
            "Shopping",
            "Healthcare",
            "Entertainment",
            "Utilities",
            "Salary",
            "Investment",
            "Gift",
            "Other"
